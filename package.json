{
  "name": "@react-hive/honey-layout",
  "version": "9.11.0",
  "description": "",
  "keywords": [
    "react",
    "layout",
    "theme",
    "responsive"
  ],
  "homepage": "https://react-honey-layout.netlify.app",
  "bugs": {
    "url": "https://github.com/React-Hive/honey-layout/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/React-Hive/honey-layout.git"
  },
  "author": "Mike Aliinyk <m.aliynik@gmail.com>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky",
    "start": "NODE_ENV=development webpack serve --config webpack-docs.config.mjs",
    "test": "jest --collect-coverage",
    "build:lib": "webpack --config webpack.config.mjs",
    "build:docs": "webpack --config webpack-docs.config.mjs",
    "diagnostics": "tsx scripts/collect-diagnostics.ts"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "files": [
    "dist",
    "!dist/**/__mocks__",
    "!dist/**/__tests__",
    "!dist/**/jest*"
  ],
  "peerDependencies": {
    "@react-hive/honey-style": "^2.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "dependencies": {
    "@floating-ui/dom": "1.7.4",
    "@floating-ui/react": "0.27.16",
    "@react-hive/honey-utils": "3.7.0",
    "csstype": "3.2.3",
    "highlight.js": "11.11.1",
    "lodash.merge": "4.6.2",
    "lodash.throttle": "4.1.1",
    "react-router-dom": "6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "9.39.1",
    "@mdx-js/loader": "3.1.1",
    "@mdx-js/react": "3.1.1",
    "@testing-library/jest-dom": "6.9.1",
    "@testing-library/react": "16.3.0",
    "@types/jest": "29.5.14",
    "@types/lodash.merge": "4.6.9",
    "@types/lodash.throttle": "4.1.9",
    "@types/mdx": "2.0.13",
    "@types/node": "22.19.2",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.2",
    "eslint": "9.39.1",
    "eslint-plugin-react": "7.37.5",
    "html-webpack-plugin": "5.6.5",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "prettier": "3.7.4",
    "style-loader": "4.0.0",
    "ts-jest": "29.4.6",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.49.0",
    "webpack": "5.103.0",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "5.2.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "testMatch": [
      "**/src/**/__tests__/**/*.spec.ts?(x)"
    ]
  },
  "browserslist": [
    "defaults",
    "not IE 11"
  ]
}
